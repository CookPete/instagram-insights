{
  "name": "instagram-insights",
  "version": "0.0.1",
  "description": "A simple React app to generate interesting stats about your Instagram profile",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack && cp index.html dist",
    "start": "node server.js",
    "lint": "standard --verbose | snazzy",
    "test": "NODE_ENV=production mocha --require ignore-styles test --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cookpete/instagram-insights.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "instagram",
    "profile",
    "insights",
    "stats"
  ],
  "author": "Pete Cook <pete@cookpete.com> (http://github.com/cookpete)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/cookpete/instagram-insights/issues"
  },
  "homepage": "https://github.com/cookpete/instagram-insights",
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "ignore-styles": "^1.1.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.4.4",
    "sass-loader": "^3.1.2",
    "snazzy": "^2.0.1",
    "standard": "^5.4.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "deepmerge": "^0.2.10",
    "fetch-jsonp": "^1.0.0",
    "lodash.pairs": "^3.0.1",
    "ls-cache": "^0.2.3",
    "normalize.css": "^3.0.3",
    "normalizr": "^1.4.0",
    "query-string": "^3.0.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "webfontloader": "^1.6.16"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
